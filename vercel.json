{
  "version": 2,
  "installCommand": "pip install -r requirements-vercel.txt",
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": { "installCommand": "pip install -r requirements-vercel.txt" }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "app.py"
    }
  ]
}
