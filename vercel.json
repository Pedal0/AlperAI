{
  "version": 2,
  "installCommand": "pip install -r requirements-vercel.txt && rm -rf asyncio",
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r requirements-vercel.txt && rm -rf asyncio"
      }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "app.py"
    }
  ]
}
